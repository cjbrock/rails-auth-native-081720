<%= form_with url: '/login', local: true do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username, autofocus: true %><br />

    <%= f.label :password %>
    <%= f.password_field :password %><br />

    <%= f.submit "Sign Up" %>
<% end %>


<%= form_tag '/login' do %>
    <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<% end %>